{
  "name": "investor-club",
  "version": "1.0.0",
  "description": "Web app for assisting investors",
  "repository": "https://github.com/GoogleCloudPlatform/nodejs-getting-started",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "node app.js",
    "serve": "ng serve",
    "lint": "ng lint",
    "build": "ng build",
    "e2e": "rm -rf node_modules/ &&  samples test build --deploy --timeout 20m --builderProject $GCLOUD_PROJECT",
    "test": "samples test app",
    "cover": "nyc --cache npm test; nyc report --reporter=html",
    "nuke": "rm -rf node_modules/; rm -rf bower_components/; rm -rf dist/; npm install; ng build;",
    "deploy": "ng build --env=prod; gcloud app deploy"
  },
  "author": "Shivam Kejriwal",
  "contributors": [
    {
      "name": "Shivam Kejriwal",
      "email": "shivamkejriwal@gmail.com"
    }
  ],
  "cloud-repo-tools": {
    "requiresKeyFile": true,
    "requiresProjectId": true,
    "test": {
      "app": {
        "args": [
          "app.js"
        ],
        "msg": "Hello, world!"
      }
    }
  },
  "license": "Apache-2.0",
  "semistandard": {
    "globals": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "it"
    ]
  },
  "dependencies": {
    "@angular/animations": "^4.4.4",
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/http": "^4.4.4",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/platform-server": "^4.4.4",
    "@angular/router": "^4.4.4",
    "express": "~4.15.2",
    "gcloud": "^0.37.2",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14",
    "body-parser": "1.16.1"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.5",
    "@angular/compiler-cli": "^4.4.4",
    "@angular/language-service": "^4.2.4",
    "@google-cloud/nodejs-repo-tools": "1.4.16",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "ava": "~0.21.0",
    "bower": "^1.8.2",
    "codelyzer": "~3.2.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "supertest": "~3.0.0",
    "tap-dot": "~1.0.5",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": ">=2.1.0 <2.4.0"
  },
  "engines": {
    "node": "~6"
  }
}
